{
  "name": "naorm-sqlite",
  "version": "0.0.1",
  "description": "A Command-Line Interface that generates TypeScript code from SQLite files in your code base.",
  "main": "dist/index.js",
  "bin": {
    "naorm": "dist/index.js"
  },
  "scripts": {
    "start": "npx ts-node src/index.ts generate",
    "build": "npx tsc && npm run json",
    "json": "typescript-json-schema ./tsconfig.json NAORMConfig -o dist/naorm-config.schema.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist"
  ],
  "author": "Matthew Moran",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.3",
    "@types/glob": "^8.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "typescript-json-schema": "^0.55.0"
  },
  "dependencies": {
    "better-sqlite3": ">=5.2.0",
    "camel-case": "~4.1.2",
    "commander": "~9.4.1",
    "dbgate-query-splitter": "~4.8.4",
    "enquirer": "~2.3.6",
    "glob": "~8.0.3"
  }
}
